<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - AdminArea</title>
    @Styles.Render("~/AdminContent/css")
    @Scripts.Render("~/bundles/modernizr")
   
    @Scripts.Render("~/bundles/bootstrap")
    <link href="@Url.Content("/Areas/AdminTheme/Content/plugins/morris.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("/Areas/AdminTheme/Content/font-awesome-4.1.0/css/font-awesome.min.css")" rel="stylesheet" type="text/css"/>
    <link href="~/Content/css/Style.css" rel="stylesheet" />
    <link href="~/Areas/AdminTheme/Content/plugins/dataTables.bootstrap.css" rel="stylesheet"/>
    <link href="~/Areas/AdminTheme/Content/css/animate.min.css" rel="stylesheet" />
    <link href="~/Areas/AdminTheme/Content/css/charisma-app.css" rel="stylesheet" />
</head>

<body>
<div id="wrapper">
    @Html.Partial("_Navbar")
    <div id="page-wrapper" class="container">
       @RenderBody()
    </div>
</div>
   
    <script src="~/Content/js/jquery-ui.js"></script>
    <script src="~/Areas/AdminTheme/Scripts/plugins/jquery.dataTables.js"></script>
    <script src="~/Areas/AdminTheme/Scripts/plugins/dataTables.bootstrap.min.js"></script>
    @RenderSection("scripts", required: false)

<script type="text/javascript">
    $(document).ready(function() {
        $('#myTable').DataTable();
    });
</script>
</body>
</html>