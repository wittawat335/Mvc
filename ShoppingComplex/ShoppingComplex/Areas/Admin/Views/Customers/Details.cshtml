@model ShoppingComplex.Models.Customer

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div class="form-horizontal">

    <div class="form-group">
        <label class="control-label col-md-3">ID </label>
        <div class="col-md-4">
            @Html.TextBoxFor(model => model.Id, new { @class = "form-control required", @disabled = "disabled" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-3">Password </label>
        <div class="col-md-4">
            @Html.TextBoxFor(model => model.Password, new { @class = "form-control required", @disabled = "disabled" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-3">E-mail </label>
        <div class="col-md-4">
            @Html.TextBoxFor(model => model.Email, new { @class = "form-control required", @disabled = "disabled" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-3">Address </label>
        <div class="col-md-4">
            @Html.TextBoxFor(model => model.Address, new { @class = "form-control required", @disabled = "disabled" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-3">Tel</label>
        <div class="col-md-4">
            @Html.TextBoxFor(model => model.Tel, new { @class = "form-control required", @disabled = "disabled" })
        </div>
    </div>
</div>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
